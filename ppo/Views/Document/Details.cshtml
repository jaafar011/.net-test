@model DAL.Entity.Documentss

<div class="card">
    <div class="card-body">
        <h5 class="card-title">Détails du Document</h5>
        <dl class="row">
            <dt class="col-sm-2">ID :</dt>
            <dd class="col-sm-10">@Model.Id</dd>

            <dt class="col-sm-2">Nom :</dt>
            <dd class="col-sm-10">@Model.Nom</dd>

            <dt class="col-sm-2">Date :</dt>
            <dd class="col-sm-10">@Model.Date</dd>

            <dt class="col-sm-2">Type :</dt>
            <dd class="col-sm-10">@Model.IdType</dd>

            <dt class="col-sm-2">Taille :</dt>
            <dd class="col-sm-10">@Model.Taille</dd>

            <dt class="col-sm-2">Catégorie :</dt>
            <dd class="col-sm-10">@Model.IdCategorie</dd>
        </dl>
    </div>
</div>
