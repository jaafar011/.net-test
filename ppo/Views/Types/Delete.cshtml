@model DAL.Entity.Typess

<h2>Confirmez la suppression du type : @Model.Nom</h2>

<form asp-controller="Types" asp-action="DeleteConfirmed" asp-route-id="@Model.Id" method="post">

    <button type="submit" class="btn btn-danger">Confirmer la suppression </button>
    <a asp-controller="Types" asp-action="Index" class="btn btn-secondary">Annuler</a>
</form>
